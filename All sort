import array as arr
import time
import matplotlib.pyplot as plt
from random import randint
x=[]
y=[]
def selectionSort(array, size):
s=time.time()
for i in range(size):
min_index = i
for j in range(i + 1, size):
if array[j] < array[min_index]:
min_index = j
(array[i], array[min_index])=(array[min_index], array[i])
e=time.time()
y.append(e-s)
x.append(arr[i])
arr=[randint(1,5000)for i in range(5000)]
size = len(arr)
selectionSort(arr, size)
print('The array after sorting in Ascending Order by selection sort is:')
print(arr)
plt.xlabel('x -length of elements arry')
plt.ylabel('y -time complexity')
plt.plot(x,y)
plt.show()
INSERTION SORT
import matplotlib.pyplot as plt
from random import randint
import time
x=[]
y=[]
def insertionSort(arr):
if (n := len(arr)) <= 1:
return
s=time.time()
for i in range(1, n):
key = arr[i]
j = i-1
while j >=0 and key < arr[j] :
arr[j+1] = arr[j]
j -= 1
arr[j+1] = key
e=time.time()
y.append(e-s)
x.append(i)

#sorting the array [12, 11, 13, 5, 6] using insertionSort
arr=[randint(1,5000)for i in range(5000)]
insertionSort(arr)
print(arr)
plt.xlabel('x -length of elements arry')
plt.ylabel('y -time complexity')
plt.plot(x,y)
plt.show()
